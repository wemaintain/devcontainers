{
  "id": "cdk",
  "name": "cdk",
  "version": "2025.5.12",
  "installsAfter": [
    "ghcr.io/lifadev/devcontainers/features/node20",
    "ghcr.io/lifadev/devcontainers/features/node23"
  ],
  "containerEnv": {
    "JSII_SILENCE_WARNING_UNTESTED_NODE_VERSION": "1"
  },
  "customizations": {
    "manifest": {
      "dependencies": [
        {
          "name": "cdk",
          "version": "2.1014.0",
          "hint": "tags/aws-cdk@v?[0-9]+\\.[0-9]+(\\.[0-9]+)?",
          "repository": "github.com/aws/aws-cdk-cli"
        }
      ]
    }
  }
}
